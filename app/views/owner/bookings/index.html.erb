<h1>Dashboard</h1>

<h2>Les elfes que j'ai réservés</h2>

<% @bookings_as_renter.each do |booking| %>
  <%= booking.elf.name %><br>
  <%= booking.status %><br>
  <%= booking.checkin %><br>
  <%= booking.checkout %><br>
<% end %>

<h2>Les réservations de mes elfes</h2>

<% @elves.each do |elf| %>
  <% elf.bookings.each do |booking| %>
  <%= booking.elf.name %><br>
  <%= booking.status %><br>
  <%= booking.checkin %><br>
  <%= booking.checkout %><br>
  <% end %>
<% end %>
